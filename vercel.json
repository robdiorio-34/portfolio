{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/scripts/(.*)",
      "dest": "/public/scripts/$1"
    },
    {
      "src": "/styles/(.*)",
      "dest": "/public/styles/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/components/(.*)",
      "dest": "/public/components/$1"
    },
    {
      "src": "/reading.html",
      "dest": "/public/pages/reading.html"
    },
    {
      "src": "/notes.html",
      "dest": "/public/pages/notes.html"
    },
    {
      "src": "/comments.html",
      "dest": "/public/pages/comments.html"
    },
    {
      "src": "/resume.html",
      "dest": "/public/pages/resume.html"
    },
    {
      "src": "/projects.html",
      "dest": "/public/pages/projects.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
